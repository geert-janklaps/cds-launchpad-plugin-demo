{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "es2022": true,
        "node": true,
        "jest": true,
        "mocha": true
    },
    "globals": {
        "SELECT": true,
        "INSERT": true,
        "UPDATE": true,
        "DELETE": true,
        "CREATE": true,
        "DROP": true,
        "cds": true
    },
    "rules": {
        "no-console": "off",
        "require-atomic-updates": "off",
        "require-await":"warn",
        "no-unused-vars": ["warn", { "argsIgnorePattern": "_" }]
    }
}
